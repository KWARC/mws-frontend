<mq:query xmlns:mq="http://mathweb.org/MathQuery">
  <math xmlns="http://www.w3.org/1998/Math/MathML">

    <apply>
        <eq mq:anyorder="yes"/>
        <apply>
            <times/>
            <ci mq:generic="1"> </ci>
            <apply>
                <plus/>
                <ci mq:generic="2"> </ci>
                <ci mq:generic="3"> </ci>
            </apply>
        </apply>

        <apply>
            <plus/>
            <apply>
                <times/>
                <ci mq:generic="1"> </ci>
                <ci mq:generic="2"> </ci>
            </apply>
            <apply>
                <times/>
                <ci mq:generic="1"> </ci>
                <ci mq:generic="3"> </ci>
            </apply>
        </apply>
    </apply>

  </math>
</mq:query>
